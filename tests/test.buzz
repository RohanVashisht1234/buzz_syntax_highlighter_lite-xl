import "std";

| A syntax highlighter for buzz programming language in lite-xl!
object Person{
    str name,
    int age,

    fun growUp() > int {
        this.age = this.age + 1;
        return this.age;
    }
}

fun main([ str ] _) > void {
    var me = Person{
        name = "Rohan Vashisht",
        age = 18,
    };
    std.print("Hello, I'm {me.name} and I'm {me.age}.");
}
